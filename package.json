{
  "name": "@hydre/graphql-websocket",
  "version": "1.0.6",
  "description": "A Websocket GraphQL server and client",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "node --harmony --experimental-specifier-resolution=node test/index.js",
    "test:debug": "DEBUG='gql-ws*' node --harmony --experimental-specifier-resolution=node test/index.js",
    "exemple:client:simple": "DEBUG='gql-ws* client*' node --harmony --experimental-specifier-resolution=node example/client/simple.js",
    "exemple:server:koa": "DEBUG='gql-ws* server*' node --harmony --experimental-specifier-resolution=node example/server/koa.js",
    "lint": "eslint --color .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/graphql-websocket.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/graphql-websocket/issues"
  },
  "keywords": [
    "graphql",
    "server",
    "client",
    "websocket"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hydreio/graphql-websocket#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "graphql": "^15.0.0",
    "koa-compose": "^4.1.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@hydre/doubt": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "casual": "^1.6.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "koa": "^2.11.0",
    "supports-color": "^7.1.0",
    "tap-spec-emoji": "^6.0.7"
  }
}
