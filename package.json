{
  "name": "@hydre/graphql-websocket",
  "version": "1.0.6",
  "description": "A blazing fast graphql websocket server and client",
  "type": "module",
  "scripts": {
    "test": "node test/index.js",
    "example:server:koa": "node example/server/koa.js",
    "example:client:simple": "node example/client/simple.js",
    "profile": "node --prof test/directives.js",
    "profile:flame": "node --prof-process --preprocess -j isolate*.log | flamebearer",
    "format": "prettier-eslint $PWD/\"**/*.js\" --write",
    "format:check": "prettier-eslint $PWD/\"**/*.js\" --list-different",
    "lint": "eslint --color .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/graphql-websocket.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/graphql-websocket/issues"
  },
  "keywords": [
    "graphql",
    "websocket",
    "server",
    "client"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hydreio/graphql-websocket#readme",
  "dependencies": {
    "@hydre/graphql-batch-executor": "^1.1.5",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "event-iterator": "^1.2.0",
    "graphql": "^15.0.0",
    "invariant": "^2.2.4",
    "koa-compose": "^4.1.0",
    "readable-stream": "^3.6.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@hydre/doubt": "^3.3.2",
    "@sidy/eslint-config-sidy": "^1.3.11",
    "babel-eslint": "^10.1.0",
    "benchmark": "^2.1.4",
    "casual": "^1.6.2",
    "eslint": "^6.8.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "koa": "^2.11.0",
    "prettier-eslint-cli": "^5.0.0",
    "supports-color": "^7.1.0",
    "tap-spec-emoji": "^6.1.1"
  }
}
