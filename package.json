{
  "name": "@hydre/shimio-graphql",
  "version": "1.0.6",
  "description": "A GraphQL server and client built on Shimio",
  "type": "module",
  "scripts": {
    "test": "node test/index.js",
    "coverage": "c8 --check-coverage node test/index.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --write",
    "postversion": "git push && git push --tags",
    "example": "node example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/shimio-graphql.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/shimio-graphql/issues"
  },
  "keywords": [
    "graphql",
    "shimio",
    "server",
    "client"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hydreio/shimio-graphql#readme",
  "dependencies": {
    "@hydre/graphql-batch-executor": "^1.2.3",
    "graphql": "^15.0.0",
    "folktale": "^2.3.2"
  },
  "devDependencies": {
    "@hydre/doubt": "^6.1.0",
    "@hydre/shimio": "^1.2.4",
    "@sidy/eslint-config-sidy": "^1.4.10",
    "babel-eslint": "^10.1.0",
    "c8": "^7.1.2",
    "casual": "^1.6.2",
    "eslint": "^7.0.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "koa": "^2.12.0",
    "prettier-eslint-cli": "^5.0.0",
    "supports-color": "^7.1.0",
    "tap-spec-emoji": "^6.1.2",
    "ws": "^7.3.0"
  }
}
