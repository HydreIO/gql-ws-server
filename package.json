{
  "name": "@hydre/wuhan",
  "version": "1.0.6",
  "description": "A transform stream implementation as a GraphQL server and client",
  "type": "module",
  "scripts": {
    "test": "node test/index.js",
    "coverage": "c8 --check-coverage node test/index.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --write",
    "example:server:koa": "node example/server/koa.js",
    "example:client:simple": "node example/client/simple.js",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/wuhan.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/wuhan/issues"
  },
  "keywords": [
    "graphql",
    "stream",
    "server",
    "client"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hydreio/wuhan#readme",
  "dependencies": {
    "@hydre/graphql-batch-executor": "^1.2.3",
    "@hydre/shimio": "^1.2.1",
    "debug": "^4.1.1",
    "folktale": "^2.3.2",
    "graphql": "^15.0.0"
  },
  "devDependencies": {
    "@hydre/doubt": "^6.1.0",
    "@sidy/eslint-config-sidy": "^1.4.8",
    "babel-eslint": "^10.1.0",
    "c8": "^7.1.2",
    "casual": "^1.6.2",
    "eslint": "^7.0.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "koa": "^2.11.0",
    "prettier-eslint-cli": "^5.0.0",
    "supports-color": "^7.1.0",
    "tap-spec-emoji": "^6.1.2"
  }
}
